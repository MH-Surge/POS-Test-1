<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="popUpImageSearchVM" type="com.application.app.modules.popupimagesearch.data.viewmodel.PopUpImageSearchVM"/>
    </data>
    <FrameLayout android:id="@+id/framePopUpImageSe"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnclose"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_23pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_23pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowclose"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageClose"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_close_25x25"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_close_25x25"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtChooseanImage"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_84pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_weight="0.87"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{popUpImageSearchVM.popUpImageSearchModel.txtChooseanImage}"
                        style="@style/txtPoppinssemibold18_1" tools:text="@string/lbl_choose_an_image"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnrectangle100"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_37pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearRowrectangle100"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageRectangle100"
                        android:layout_height="@dimen/_127pxh"
                        android:layout_width="@dimen/_133pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle100}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle100"/>
                    <ImageView android:id="@+id/imageRectangle101"
                        android:layout_height="@dimen/_127pxh"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_width="@dimen/_187pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle101}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle101"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowrectangle103"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageRectangle103"
                        android:layout_height="@dimen/_127pxh"
                        android:layout_marginBottom="@dimen/_35pxv"
                        android:layout_marginTop="@dimen/_25pxv"
                        android:layout_width="@dimen/_187pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle103}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle103"/>
                    <ImageView android:id="@+id/imageRectangle102"
                        android:layout_height="@dimen/_187pxh"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_width="@dimen/_127pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle102}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle102"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowrectangle105"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageRectangle105"
                        android:layout_height="@dimen/_187pxh"
                        android:layout_width="@dimen/_127pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle105}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle105"/>
                    <ImageView android:id="@+id/imageRectangle104"
                        android:layout_height="@dimen/_127pxh"
                        android:layout_marginBottom="@dimen/_60pxv"
                        android:layout_marginStart="@dimen/_41pxh"
                        android:layout_width="@dimen/_171pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle104}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle104"/>
                </LinearLayout>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnLoadMore"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_77pxh"
                android:layout_marginStart="@dimen/_77pxh"
                android:layout_marginTop="@dimen/_29pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_12pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_12pxv"
                android:singleLine="true"
                android:text="@string/lbl_load_more"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_14" tools:text="@string/lbl_load_more"/>
        </LinearLayout>
    </FrameLayout>
</layout>
