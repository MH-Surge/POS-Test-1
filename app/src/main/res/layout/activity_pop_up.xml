<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="popUpVM" type="com.application.app.modules.popup.data.viewmodel.PopUpVM"/>
    </data>
    <FrameLayout android:id="@+id/framePopUp"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumncancel"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_33pxh"
            android:layout_marginStart="@dimen/_33pxh"
            android:layout_marginTop="@dimen/_43pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowcancel"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCancel"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.21"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{popUpVM.popUpModel.txtCancel}"
                        style="@style/txtPoppinssemibold14" tools:text="@string/lbl_cancel"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCompletePaymen"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_33pxh"
                        android:layout_weight="0.79"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{popUpVM.popUpModel.txtCompletePaymen}"
                        style="@style/txtPoppinsregular20" tools:text="@string/msg_complete_paymen"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <TextView android:ellipsize="end"
                android:id="@+id/txtCardPayment"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_46pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{popUpVM.popUpModel.txtCardPayment}"
                style="@style/txtPoppinssemibold18" tools:text="@string/lbl_card_payment"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtCompletetheOr"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{popUpVM.popUpModel.txtCompletetheOr}"
                style="@style/txtPoppinsregular16_2" tools:text="@string/msg_complete_the_or"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtBillTotal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_67pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{popUpVM.popUpModel.txtBillTotal}"
                style="@style/txtPoppinsregular18" tools:text="@string/lbl_bill_total"/>
            <LinearLayout android:id="@+id/linearRowprice"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylecornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPrice"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_weight="0.34"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{popUpVM.popUpModel.txtPrice}"
                    style="@style/txtPoppinsregular18" tools:text="@string/lbl_mwk"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txt11895000"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginEnd="@dimen/_35pxh"
                    android:layout_marginStart="@dimen/_176pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_weight="0.66"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{popUpVM.popUpModel.txt11895000}"
                    style="@style/txtPoppinsregular18" tools:text="@string/lbl_118_950_00"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtGratuity"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_88pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{popUpVM.popUpModel.txtGratuity}"
                style="@style/txtPoppinsregular18" tools:text="@string/lbl_gratuity"/>
            <EditText
                android:background="@drawable/rectangle_border_gray_900_radius_10"
                android:ellipsize="end"
                android:hint="@string/lbl_enter_an_amount"
                android:id="@+id/etGroup162" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_15pxv"
                android:singleLine="true"
                android:text="@={popUpVM.popUpModel.etGroup162Value}"
                android:textColorHint="@color/gray_900_8e"
                style="@style/etRoundedOutline_1" tools:ignore="Autofill"/>
            <View android:background="@color/white_A700"
                android:id="@+id/viewRectangleEightyTwo"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_37pxh"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_39pxv" android:layout_width="@dimen/_37pxh"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnConfirm"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_75pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_confirm"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/lbl_confirm"/>
        </LinearLayout>
    </FrameLayout>
</layout>
