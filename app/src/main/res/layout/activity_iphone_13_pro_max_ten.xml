<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="iphone13ProMaxTenVM" type="com.application.app.modules.iphone13promaxten.data.viewmodel.Iphone13ProMaxTenVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearIPhone13ProM"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnmenu"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_33pxh"
                android:layout_marginStart="@dimen/_39pxh"
                android:layout_marginTop="@dimen/_56pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowmenu"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageMenu"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_menu"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_menu"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLinQPOS"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginStart="@dimen/_92pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="0.62"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{iphone13ProMaxTenVM.iphone13ProMaxTenModel.txtLinQPOS}"
                        style="@style/txtPoppinssemibold25_1" tools:text="@string/lbl_linq_pos"/>
                    <ImageView android:id="@+id/imageRectangleSixtySix"
                        android:layout_height="@dimen/_40pxh"
                        android:layout_marginStart="@dimen/_85pxh"
                        android:layout_width="@dimen/_40pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_5pxh}"
                        imageRes="@{@drawable/img_rectangle66}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle66"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowarrowone"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_36pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageArrowOne"
                android:layout_height="@dimen/_2pxh"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="@dimen/_1pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow1"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow1"/>
            <TextView android:ellipsize="end" android:id="@+id/txtBack"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_9pxv"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_weight="0.13" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{iphone13ProMaxTenVM.iphone13ProMaxTenModel.txtBack}"
                style="@style/txtPoppinsregular12_1" tools:text="@string/lbl_back"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtCompletePaymen"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_33pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.87" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{iphone13ProMaxTenVM.iphone13ProMaxTenModel.txtCompletePaymen}"
                style="@style/txtPoppinssemibold20" tools:text="@string/msg_complete_paymen"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnmobile"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_31pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_1">
            <ImageView android:id="@+id/imageMobile"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_41pxh"
                android:layout_marginEnd="@dimen/_120pxh"
                android:layout_marginStart="@dimen/_120pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_width="@dimen/_37pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_mobile"
                tools:ignore="ContentDescription" tools:src="@drawable/img_mobile"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtMobileMoney"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_120pxh"
                android:layout_marginStart="@dimen/_120pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{iphone13ProMaxTenVM.iphone13ProMaxTenModel.txtMobileMoney}"
                style="@style/txtPoppinsregular18_2" tools:text="@string/lbl_mobile_money"/>
        </LinearLayout>
        <TextView android:ellipsize="end" android:id="@+id/txtGratuity"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_43pxh"
            android:layout_marginStart="@dimen/_43pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{iphone13ProMaxTenVM.iphone13ProMaxTenModel.txtGratuity}"
            style="@style/txtPoppinsregular18_3" tools:text="@string/lbl_gratuity"/>
        <EditText
            android:background="@drawable/rectangle_border_gray_900_radius_10"
            android:ellipsize="end"
            android:hint="@string/lbl_enter_an_amount"
            android:id="@+id/etGroup161" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_16pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_20pxh"
            android:paddingTop="@dimen/_15pxv" android:singleLine="true"
            android:text="@={iphone13ProMaxTenVM.iphone13ProMaxTenModel.etGroup161Value}"
            android:textColorHint="@color/gray_900_8e"
            style="@style/etRoundedOutline_1" tools:ignore="Autofill"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtAirtelMoneyNu"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_70pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{iphone13ProMaxTenVM.iphone13ProMaxTenModel.txtAirtelMoneyNu}"
            style="@style/txtPoppinssemibold18_1" tools:text="@string/msg_airtel_money_nu"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txt09900000000"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{iphone13ProMaxTenVM.iphone13ProMaxTenModel.txt09900000000}"
            style="@style/txtPoppinssemibold20" tools:text="@string/lbl_09900000000"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnConfirm"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_238pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_16pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_16pxv" android:singleLine="true"
            android:text="@string/lbl_confirm"
            android:textAllCaps="false" style="@style/btnSolidRounded_1" tools:text="@string/lbl_confirm"/>
    </LinearLayout>
</layout>
