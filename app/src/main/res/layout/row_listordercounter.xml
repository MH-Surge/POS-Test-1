<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listordercounterRowModel" type="com.application.app.modules.iphone13promaxthree.data.model.ListordercounterRowModel"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnordercounter"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="vertical" style="@style/groupStylewhite_1">
        <TextView android:ellipsize="end"
            android:id="@+id/txtOrderCounter"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{listordercounterRowModel.txtOrderCounter}"
            style="@style/txtPoppinssemibold20" tools:text="@string/lbl_order_001"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtItemsCounter"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{listordercounterRowModel.txtItemsCounter}"
            style="@style/txtPoppinsregular20" tools:text="@string/lbl_18_items"/>
        <LinearLayout android:id="@+id/linearRowtotal"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_47pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <TextView android:ellipsize="end" android:id="@+id/txtTotal"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_weight="0.24" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{listordercounterRowModel.txtTotal}"
                style="@style/txtPoppinsregular18" tools:text="@string/lbl_total2"/>
            <TextView android:ellipsize="end" android:id="@+id/txtPrice"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_116pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.76" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{listordercounterRowModel.txtPrice}"
                style="@style/txtPoppinssemibold18_1" tools:text="@string/lbl_mwk_118_750_00"/>
        </LinearLayout>
    </LinearLayout>
</layout>
