<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="addNewItemToInventoryPopUpVM" type="com.application.app.modules.addnewitemtoinventorypopup.data.viewmodel.AddNewItemToInventoryPopUpVM"/>
    </data>
    <FrameLayout android:id="@+id/frameAddNewItemto"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnclose"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_36pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowclose"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageClose"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_close_25x25"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_close_25x25"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAdd"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_139pxh"
                        android:layout_weight="0.68"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{addNewItemToInventoryPopUpVM.addNewItemToInventoryPopUpModel.txtAdd}"
                        style="@style/txtPoppinssemibold25" tools:text="@string/lbl_add"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtThisIngredient"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_37pxh"
                android:layout_marginStart="@dimen/_37pxh"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_width="match_parent"
                android:text="@{addNewItemToInventoryPopUpVM.addNewItemToInventoryPopUpModel.txtThisIngredient}"
                style="@style/txtPoppinsregular18" tools:text="@string/msg_this_ingredient"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_gray_900_radius_6_1"
                android:ellipsize="end"
                android:hint="@string/lbl_lettuce"
                android:id="@+id/etGroupThirtyThree"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_89pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_19pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text="@={addNewItemToInventoryPopUpVM.addNewItemToInventoryPopUpModel.etGroupThirtyThreeValue}"
                android:textColorHint="@color/gray_900"
                style="@style/etSolidRoundedOutline_4" tools:ignore="Autofill"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnDoNotAdd"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_155pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text="@string/lbl_do_not_add"
                android:textAllCaps="false"
                style="@style/btnRoundedOutline_4" tools:text="@string/lbl_do_not_add"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnAddOne"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true" android:text="@string/lbl_add"
                android:textAllCaps="false"
                style="@style/btnSolidRoundedOutline_4" tools:text="@string/lbl_add"/>
        </LinearLayout>
    </FrameLayout>
</layout>
