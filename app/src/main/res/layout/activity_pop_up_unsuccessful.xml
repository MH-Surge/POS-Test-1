<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="popUpUnsuccessfulVM" type="com.application.app.modules.popupunsuccessful.data.viewmodel.PopUpUnsuccessfulVM"/>
    </data>
    <FrameLayout android:id="@+id/framePopUpUnsucces"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumncancel"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_33pxh"
            android:layout_marginStart="@dimen/_33pxh"
            android:layout_marginTop="@dimen/_43pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowcancel"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCancel"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.21"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{popUpUnsuccessfulVM.popUpUnsuccessfulModel.txtCancel}"
                        style="@style/txtPoppinssemibold14" tools:text="@string/lbl_cancel"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCompletePaymen"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_33pxh"
                        android:layout_weight="0.79"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{popUpUnsuccessfulVM.popUpUnsuccessfulModel.txtCompletePaymen}"
                        style="@style/txtPoppinsregular20" tools:text="@string/msg_complete_paymen"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <TextView android:ellipsize="end" android:id="@+id/txtOhNo"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_233pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{popUpUnsuccessfulVM.popUpUnsuccessfulModel.txtOhNo}"
                style="@style/txtPoppinsregular40" tools:text="@string/lbl_oh_no"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtYourPaymentWa"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{popUpUnsuccessfulVM.popUpUnsuccessfulModel.txtYourPaymentWa}"
                style="@style/txtPoppinsregular20" tools:text="@string/msg_your_payment_wa"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnTryAgain"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_163pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_14pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@string/lbl_try_again"
                android:textAllCaps="false"
                style="@style/btnRoundedOutline" tools:text="@string/lbl_try_again"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnDone"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_39pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_done"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/lbl_done"/>
        </LinearLayout>
    </FrameLayout>
</layout>
