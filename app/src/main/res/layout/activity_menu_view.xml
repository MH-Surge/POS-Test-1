                        android:text="@{menuViewVM.menuViewModel.txtPriceOne}"
                        style="@style/txtPoppinssemibold16" tools:text="@string/lbl_mwk_30_000_00"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRoweditOne"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_900">
                <TextView android:ellipsize="end"
