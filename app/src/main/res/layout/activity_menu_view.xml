                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearRowclose"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.99"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtClose"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.2"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{menuViewVM.menuViewModel.txtClose}"
                            style="@style/txtPoppinssemibold16_1" tools:text="@string/lbl_close"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtOrderNumberZero"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_45pxh"
                            android:layout_weight="0.8"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{menuViewVM.menuViewModel.txtOrderNumberZero}"
                            style="@style/txtPoppinsregular20_1" tools:text="@string/msg_order_number_00"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowThree"
                        android:layout_height="@dimen/_1pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginStart="@dimen/_77pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="@dimen/_2pxh"
