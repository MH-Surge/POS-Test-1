                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.9"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{menuViewVM.menuViewModel.txtMainChickenS}"
                        style="@style/txtPoppinsregular16" tools:text="@string/msg_main_chicken_s"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtXCounterOne"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_103pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.1"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{menuViewVM.menuViewModel.txtXCounterOne}"
