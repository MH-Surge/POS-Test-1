                <LinearLayout android:id="@+id/linearRow250000"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:layout_marginEnd="@dimen/_31pxh"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt250000"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.35"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{menuViewVM.menuViewModel.txt250000}"
                        style="@style/txtPoppinsregular16" tools:text="@string/lbl_2_500_00"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPrice"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_178pxh"
                        android:layout_weight="0.65"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{menuViewVM.menuViewModel.txtPrice}"
                        style="@style/txtPoppinssemibold16" tools:text="@string/lbl_mwk_17_500_00"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRowedit"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_900">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEdit"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:layout_marginStart="@dimen/_93pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_weight="0.12"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{menuViewVM.menuViewModel.txtEdit}"
                    style="@style/txtPoppinsregular16_3" tools:text="@string/lbl_edit"/>
