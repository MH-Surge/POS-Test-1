                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnDeleteOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_92pxh"
                    android:layout_weight="0.88"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_17pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_delete"
                    android:textAllCaps="false" style="@style/btnSolid" tools:text="@string/lbl_delete"/>
            </LinearLayout>
            <EditText android:ellipsize="end"
                android:hint="@string/lbl_reciept_preview"
                android:id="@+id/etGroup210" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_marginTop="@dimen/_56pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_9pxh"
                android:singleLine="true"
                android:text="@={menuViewVM.menuViewModel.etGroup210Value}"
                android:textColorHint="@color/gray_900"
                style="@style/etPoppinsregular18" tools:ignore="Autofill"/>
            <LinearLayout android:id="@+id/linearRowpriceTwo"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowvat"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.41"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtVAT"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.45"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{menuViewVM.menuViewModel.txtVAT}"
                        style="@style/txtPoppinsregular18" tools:text="@string/lbl_vat"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtOneHundredSixtyFive"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_weight="0.55"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{menuViewVM.menuViewModel.txtOneHundredSixtyFive}"
                        style="@style/txtPoppinsregular18" tools:text="@string/lbl_16_5"/>
