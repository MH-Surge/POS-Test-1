<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="popUpOneVM" type="com.application.app.modules.popupone.data.viewmodel.PopUpOneVM"/>
    </data>
    <FrameLayout android:id="@+id/framePopUpOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumncancel"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_33pxh"
            android:layout_marginStart="@dimen/_33pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowcancel"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCancel"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_weight="0.27"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{popUpOneVM.popUpOneModel.txtCancel}"
                        style="@style/txtPoppinssemibold14" tools:text="@string/lbl_cancel"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEnterYourPin"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_63pxh"
                        android:layout_weight="0.73"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{popUpOneVM.popUpOneModel.txtEnterYourPin}"
                        style="@style/txtPoppinsregular20" tools:text="@string/lbl_enter_your_pin"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <FrameLayout android:id="@+id/frameStackgroup277"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_51pxv"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_44pxv"
                android:layout_width="@dimen/_311pxh" style="@style/groupStylegray_200cornerRadius">
                <ImageView android:id="@+id/imageGroup277"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_141pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group277"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group277"/>
            </FrameLayout>
            <FrameLayout android:id="@+id/frameStackrectangleeightytwo"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_413pxv"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_35pxv" android:layout_width="@dimen/_311pxh">
                <View android:background="@color/white_A700"
                    android:id="@+id/viewRectangleEightyTwo"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_37pxh"
                    android:layout_marginBottom="@dimen/_40pxv"
                    android:layout_marginEnd="@dimen/_137pxh"
                    android:layout_marginStart="@dimen/_137pxh"
                    android:layout_marginTop="@dimen/_40pxv" android:layout_width="@dimen/_37pxh"/>
                <LinearLayout android:id="@+id/linearColumngroup269"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/recyclerPopUpOne"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        itemSpace="@{@dimen/_23pxh}" tools:itemCount="3" tools:listitem="@layout/row_pop_up_one"/>
                    <LinearLayout
                        android:id="@+id/linearRowrectangle115"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_23pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <View
                            android:background="@drawable/rectangle_bg_gray_900_radius_7"
                            android:id="@+id/viewRectangle115"
                            android:layout_height="@dimen/_86pxh" android:layout_width="@dimen/_86pxh"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnZero"
                            android:layout_height="@dimen/_86pxh"
                            android:layout_marginStart="@dimen/_26pxh"
                            android:layout_width="@dimen/_86pxh"
                            android:paddingHorizontal="@dimen/_30pxh"
                            android:paddingVertical="@dimen/_30pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_0"
                            android:textAllCaps="false"
                            style="@style/btnSolidRounded_7" tools:text="@string/lbl_0"/>
                        <View
                            android:background="@drawable/rectangle_bg_gray_900_radius_7"
                            android:id="@+id/viewRectangle117"
                            android:layout_height="@dimen/_86pxh"
                            android:layout_marginStart="@dimen/_26pxh" android:layout_width="@dimen/_86pxh"/>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnConfirm"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_35pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_confirm"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/lbl_confirm"/>
            <View android:background="@color/white_A700"
                android:id="@+id/viewRectangleEightyThree"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_37pxh"
                android:layout_marginEnd="@dimen/_26pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_11pxv" android:layout_width="@dimen/_37pxh"/>
        </LinearLayout>
    </FrameLayout>
</layout>
