<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="onboardingSevenVM" type="com.application.app.modules.onboardingseven.data.viewmodel.OnboardingSevenVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumncheckmark"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_129pxv"
        android:layout_marginEnd="@dimen/_23pxh"
        android:layout_marginStart="@dimen/_23pxh"
        android:layout_marginTop="@dimen/_214pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearOnboardingSeve"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageView android:id="@+id/imageCheckmark"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_60pxh"
                    android:layout_marginEnd="@dimen/_45pxh"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_width="@dimen/_60pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_checkmark_60x60"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_checkmark_60x60"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtYoureAllSet"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_45pxh"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_marginTop="@dimen/_36pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{onboardingSevenVM.onboardingSevenModel.txtYoureAllSet}"
                    style="@style/txtPoppinssemibold30_1" tools:text="@string/lbl_you_re_all_set"/>
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtNowthatyouve"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_44pxh"
                    android:layout_marginStart="@dimen/_45pxh"
                    android:layout_marginTop="@dimen/_44pxv"
                    android:layout_width="match_parent"
                    android:text="@{onboardingSevenVM.onboardingSevenModel.txtNowthatyouve}"
                    style="@style/txtPoppinsregular20" tools:text="@string/msg_now_that_you_ve"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnGetStarted"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_287pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_17pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_get_started"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_get_started"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
